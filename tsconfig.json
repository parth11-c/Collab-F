{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "esModuleInterop": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "expo-router": [
        "./node_modules/expo-router/build/index.d.ts"
      ],
      "expo-router/*": [
        "./node_modules/expo-router/build/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
